                <td>{{ medecin.nom }}</td>
                <td>{{ medecin.prenom }}</td>

                <td>{{ medecin.email }}</td>
                <td>{{ medecin.telephone }}</td>
                <td>{{ medecin.address }}</td>
                <td>{{ medecin.specialite }}</td>


                <td>
                    <a href="{{ path('medecin_show', {'id': medecin.id}) }}">afficher profil</a>

                </td>

                <td>
                    <a href="{{ path('dispo_ah_show',  {'id': dispo_ah.id}) }}">afficher profil</a>

                </td>


            </tr>
        {% else %}
            <tr>
                <td colspan="12">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="#"></a>
{% endblock %}

    <table class="table">
        <thead>
        <tr>

            <th>Nom</th>
            <th>Prenom</th>
            <th>debut</th>
            <th>fin</th>

            <th>Specialite</th>


            <th>actions</th>
        </tr>
        </thead>
        <tbody>
        {% for medecin in medecins %}
